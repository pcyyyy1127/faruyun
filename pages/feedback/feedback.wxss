/* pages/feedback/feedback.wxss */
.box {
  display: flex;
  flex-direction: column;
}
.title-box {
  text-align: left;
  font-size: 56rpx;
  color: #D09363;
  font-family: 方正工业黑-标准;
  margin: 40rpx 50rpx;
}

.con-box {
  width: 94vw;
  height: 70vh;
  margin: 10rpx 3vw;
  background-color:#FAFAFA;
}
.con-box-txt {
  color: #9D9E9F;
  font-size: 32rpx;
  margin-top: 60rpx;
  margin-left: 40rpx;
}

.con-box-area {
  width: 90%;
  height: 90%;
  color: #000000;
  font-size: 32rpx;
  margin-top: 10rpx;
  margin-left: 40rpx;
}
.con-box-btn {
  width: 70%;
  line-height: 60rpx;
  text-align: center;
  position: absolute;
  margin: 0 15%;
  font-size: 28rpx;
  bottom: 60rpx;
  color: white;
  background-color: #913D3F;
  border-radius: 30rpx;
}