page {
  color: #000;
  font-size: 30rpx;
  width: 100%;
  height: 100%;
  font-family: PingFangSC-Regular;
  background-color: #fff;
}
.back{
  position: fixed;
  width: 100%;
  height: 20vh;
  border-radius: 0  0 120rpx 120rpx ;
  background-color: #913D3F;
  z-index: -1;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fullscreen {
  width: 100%;
  height: 100%;
}
.background-wrapper {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
button[disabled]:not([type]) {
  background-color:rgb(85,188,250);
  color: #fff;
}

cover-view {
  position: absolute;
}

@import 'app-wxa-auto-dark.wxss'
